---
import PageHeader from "../components/PageHeader.astro";
import CostStructure from "../components/React/CostStructure/CostStructure";
import Create from "../components/React/CostStructure/UI/Create";
import Loader from "../components/React/UI/Loader";
import SSRCostStructure from "../components/SSRCostStructure.astro";
import Layout from "../layouts/Layout.astro";
export const prerender = true;
---

<Layout title="Estructura de costos">
  <PageHeader title="Estructura de costos" iconName="clipboard">
    <Create client:load />
  </PageHeader>
  <div class="p-6">
    <SSRCostStructure server:defer>
      <div slot="fallback">
        <Loader />
      </div>
    </SSRCostStructure>
  </div>
</Layout>
